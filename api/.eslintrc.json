{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "@vue/eslint-config-prettier"
  ],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": ["vue", "prettier"],
  "rules": {
    "vue/multi-word-component-names": "off",
    "vue/no-v-html": "off",
    "prettier/prettier": "error"
  }
}

